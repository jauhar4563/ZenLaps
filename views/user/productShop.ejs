<%- include('../Layouts/header.ejs') %>

    <main class="main">
        <nav aria-label="breadcrumb" class="breadcrumb-nav mb-2">
           
        </nav><!-- End .breadcrumb-nav -->

        <div class="page-content mt">
            <div class="container">
                <div class="row">
                    <div class="col-lg-9 col-xl-4-5col">
                     

                        <div class="mb-3"></div><!-- End .mb-3 -->

                        
                        <div class="mb-3 mb-lg-5"></div><!-- End .mb-3 mb-lg-5 -->

                        <div class="cat-blocks-container">
                            <h3 class="title title-border text-center">Categories</h3><!-- End .title -->

                            <div class="row" style="display: flex; justify-content: center;">
                                <% if (category.length > 0) { %>
                                    <% for (let i = 0; i < category.length; i++) { %>
                                        <div class="col-6 col-md-4 col-lg-2">
                                            <a href="category.html" class="cat-block">
                                                <figure>
                                                    <span>
                                                        <img src="../../Public/categoryImages/<%= category[i].image %>" alt="Category image">
                                                    </span>
                                                </figure>
                                                <h3 class="cat-block-title"><%= category[i].name %></h3><!-- End .cat-block-title -->
                                            </a>
                                        </div><!-- End .col-6 col-md-4 col-lg-3 -->
                                        <% } %>
                                        <% } else { %>
                                            <div class="col-6 col-md-4 col-lg-3">
                                                <p>User not found</p>
                                            </div>
                                        <% } %>
                            </div><!-- End .row -->
                        </div><!-- End .cat-blocks-container -->

                        <div class="mb-2"></div><!-- End .mb-2 -->




                        <div class="mb-4"></div><!-- End .mb-4 -->

                        <div class="toolbox">
                            <div class="toolbox-left">
                                <div class="toolbox-info">
                                    <%= products.length %> Products found
                                </div><!-- End .toolbox-info -->
                            </div><!-- End .toolbox-left -->

                            <div class="toolbox-right">
                                <div class="toolbox-sort">
                                    <label for="sortby">Sort by:</label>
                                    <div class="select-custom">
                                        <select name="sortby" id="sortby" class="form-control">
                                            <option value="popularity" selected="selected">Most Popular</option>
                                            <option value="rating">Most Rated</option>
                                            <option value="date">Date</option>
                                        </select>
                                    </div>
                                </div><!-- End .toolbox-sort -->
                            </div><!-- End .toolbox-right -->
                        </div><!-- End .toolbox -->

                        <div class="products mb-3">
                            <div class="row">
                            



                                <%
                                if (products.length > 0) {
                                  for (let i = 0; i < products.length; i++) {
                                    %>
                                <div class="col-6 col-md-4 col-xl-3" style="margin-left: 30px;">
                                    <div class="product" style="border-radius: 10px;">
                                        <figure class="product-media">
                                            <!-- <span class="product-label label-top">Top</span> -->
                                            <a href="/productView?id=<%= products[i]._id %>">
                                                <img src="../../Public/productImages/<%= products[i].image[0] %>" alt="Product image" class="product-image">
                                            </a>

                                            <div class="product-action-vertical" >
                                                <a href="#" style="text-decoration: none;" class="btn-product-icon btn-wishlist btn-expandable"><span>add to wishlist</span></a>
                                              
                                            </div>

                                            <div class="product-action">
                                                <a href="#" style="text-decoration: none; border-radius: 10px; width: 100%;" class="btn-product btn-cart" title="Add to cart"><span>add to cart</span></a>
                                            </div>
                                        </figure>

                                        <div class="product-body">
                                          
                                            <div class="product-cat">
                                                <a href="#"><%= products[i].brand %></a>
                                            </div>
                                            <div class="product-price">
                                                <%= products[i].discountPrice %>
                                               <span style="color: rgb(117, 117, 117); margin-left: 10px; text-decoration: line-through;"><%= products[i].price%></span> 
                                            </div>
                                            <h3 class="product-title"><a href="/productView?id=<%= products[i]._id %>"><%= products[i].name %></a></h3><!-- End .product-title -->
                                            
                                            <div class="ratings-container">
                                                <div class="ratings">
                                                    <div class="ratings-val" style="width: 100%;"></div><!-- End .ratings-val -->
                                                </div><!-- End .ratings -->
                                                <span class="ratings-text">( 4 Reviews )</span>
                                            </div>
                                     
                                        </div>
                                    </div>
                                </div>
                                <% } %>
                                <% } else { %>
                                    <div class="col-6 col-md-4 col-xl-3">
                                        <p>User not found</p>
                                    </div>
                                <% } %>
                
                              
                             
                            </div><!-- End .row -->
                        </div><!-- End .products -->

