                <div class="tab-content tab-content-carousel">
                    <div class="tab-pane p-0 fade show active" id="elec-new-tab" role="tabpanel" aria-labelledby="elec-new-link">
                        <div class="owl-carousel owl-simple carousel-equal-height carousel-with-shadow" data-toggle="owl" 
                            data-owl-options='{
                                "nav": false, 
                                "dots": true,
                                "margin": 20,
                                "loop": false,
                                "responsive": {
                                    "0": {
                                        "items":2
                                    },
                                    "480": {
                                        "items":2
                                    },
                                    "768": {
                                        "items":3
                                    },
                                    "992": {
                                        "items":4
                                    },
                                    "1280": {
                                        "items":5,
                                        "nav": true
                                    }
                                }
                            }'>

                            <% if (products.length > 0) { %>
                                <% products.filter(product => product.is_listed === true && product.discountStatus==true).forEach((product) => { %>
                                <div class="product">
                                    <figure class="product-media">
                                        <%if(product.discountStatus==true &&
                                        new Date() <= new Date(product.discountEnd) &&
                                        new Date() >= new Date(product.discountStart) ){%>

                                            <span class="product-label label-sale">Sale</span>
                                            <% } %>                                            <a href="/productView?id=<%= product._id %>">
                                            <img src="/productImages/<%= product.image[0] %>" alt="Product image" class="product-image">
                                        </a>

                                      
                                    </figure><!-- End .product-media -->

                                    <div class="product-body">
                                        <div class="product-cat">
                                            <a href="#">Clothes</a>
                                        </div><!-- End .product-cat -->
                                        <h3 class="product-title"><a href="/productView?id=<%= product._id %>"><% if (product.name.length > 110) { %>
                                            <%= product.name.slice(0, 100) + '...' %>
                                          <% } else { %>
                                            <%= product.name %> <% } %></a></h3><!-- End .product-title -->
                                        <div class="product-price">
                                            <% if (
                                                product.discountPrice !== product.price &&
                                                new Date() <= new Date(product.discountEnd) &&
                                                new Date() >= new Date(product.discountStart)
                                              ) { %>
                                            <span class="new-price"><%= product.discountPrice %></span>
                                            <span class="old-price">Was <%= product.price %></span>
                                            <% } else { %>
                                                <span class="new-price"><%= product.price %></span>
                                                <% } %>
                                        </div><!-- End .product-price -->
                                        <% if (product.ratings && product.ratings.length > 0) { %>
                                            <% let sum = product.ratings.reduce((acc, rating) => acc + rating, 0); %>
                                            <% let averageRating = sum / product.ratings.length; %>
                                            <div class="ratings-container">
                                                <div class="ratings">
                                                    <div class="ratings-val" style="width: <%= (averageRating / 5) * 100 %>%;"></div>
                                                    <!-- End .ratings-val -->
                                                </div><!-- End .ratings -->
                                                <span class="ratings-text">(<%= product.ratings.length %> Reviews)</span>
                                            </div><!-- End .ratings-container -->
                                        <% } else { %>
                                            <div class="ratings-container">
                                                <div class="ratings">
                                                    <div class="ratings-val" style="width: 0%;"></div>
                                                    <!-- End .ratings-val -->
                                                </div><!-- End .ratings -->
                                                <span class="ratings-text">(No Reviews)</span>
                                            </div><!-- End .ratings-container -->
                                        <% } %>

                                      
                                    </div><!-- End .product-body -->
                                </div><!-- End .product -->
                                <% }); %>
                                <% } else { %>
                                    <p>Products not found</p>
                                <% } %>

                            
                        </div><!-- End .owl-carousel -->
                    </div><!-- .End .tab-pane -->
           
                </div><!-- End .tab-content -->
